{
  "name": "ifkz-frontend-new",
  "version": "1.0.0",
  "description": "IFKZ new frontend ",
  "license": "MIT",
  "typings": "lib/ts/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "ci": "run-p ci:*",
    "ci:lint": "eslint --cache --max-warnings 0 src",
    "ci:test": "jest --passWithNoTests",
    "clean": "rm -rf lib",
    "compile": "yarn clean && tsc --outDir lib/ts -p src",
    "start": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": [
      "@azimutlabs"
    ]
  },
  "eslintIgnore": [
    "**/node_modules",
    "**/lib"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "eslint --no-ignore --cache --max-warnings 0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.36",
    "bootstrap-4-grid": "^3.4.0",
    "classnames": "^2.2.6",
    "emotion-rgba": "^0.0.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.9.5",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@azimutlabs/eslint-config": "^0.2.0",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "eslint": "^7.3.0",
    "husky": "^4.2.5",
    "babel-loader": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-array-func": "^3.1.6",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "prettier": "^2.0.5",
    "lint-staged": "^10.2.9",
    "react-docgen-typescript-loader": "^3.7.2",
    "ts-loader": "^8.0.1"
  }
}
